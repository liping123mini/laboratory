.ul{
  padding: 20rpx;
}
.li{
  display: flex;
  height: 80rpx;
  margin:20rpx 0;
  background-color: #fff;
  border-radius: 10rpx;
  font-size: 30rpx;
  color: #333;
  line-height: 80rpx;
  padding:0 20rpx;
  box-shadow: 0 0 10rpx 5rpx rgba(0,0,0,.1);
  align-items: center;
}
image{
  width: 100%;
}
.name{
  width: 200rpx;
}
.time{
  flex:1;
  text-align: center;
}

.icon-close{
  color: #d81e06;
}
.icon-select{
  color: #1afa29;
}




