/* //房间弹框样式 */
.bottomPicker {
	width: 100%;
	position: fixed;
	bottom: 0rpx;
	z-index: 10000;
	padding: 30rpx 0rpx 0rpx 0rpx;
	box-shadow: 0rpx 0rpx 10rpx 8rpx #ccc;
	border-radius: 50rpx 50rpx 0rpx 0rpx;
	background: rgba(255, 255, 255);
}

.bottomPicker .single {
	width: 90%;
	margin: auto;
	position: relative;
	border-radius: 20rpx;
	padding: 25rpx;
	margin-bottom: 25rpx;
	border: 1rpx solid rgba(243, 243, 243);
}
.bottomPicker .single text {
	font-size: 30rpx;
}
.bottomPicker .single::before {
	left: 30%;
	content: '';
	width: 30rpx;
	height: 30rpx;
	background: rgba(255, 255, 255);
	position: absolute;
	margin-top: -40rpx;
	transform: rotate(45deg);
	border-right: 1rpx solid rgba(243, 243, 243);
	border-bottom: 1rpx solid rgba(243, 243, 243);
}

.bottomPicker .single .left {
	width: 25%;
}

.bottomPicker .single .left text {
	display: block;
	text-align: center;
}

.bottomPicker .single .right {
	color: #666;
	width: 65%;
	font-size: 30rpx;
	line-height: 35rpx;
}
.bottomPicker .mybtns {
	padding: 0rpx 47rpx;
}
.bottomPicker .mybtns text {
	padding: 0rpx 20rpx;
	line-height: 65rpx;
	font-size: 30rpx;
}
