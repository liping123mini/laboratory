<cu-custom bgColor="bg-gradual-green" isBack="{{true}}">
  <view slot="backText">返回</view>
  <view slot="content">体温</view>
</cu-custom>
<form  bindsubmit="formSubmit">
	<view class="cu-form-group margin-top">
		<view class="title">姓名</view>
		<input value='{{username}}' name="username" disabled='true'></input>
	</view>
	<view class="cu-form-group margin-top">
		<view class="title">学院</view>
		<input value='{{collegeName}}' name="collegeName" disabled='true'></input>
	</view>
	<view class="cu-form-group margin-top">
		<view class="title">班级</view>
		<input value='{{className}}' name="className" disabled='true'></input>
	</view>
	<view class="cu-form-group margin-top">
		<view class="title">学号</view>
		<input value='{{studentNum}}' name="studentNum" disabled='true'></input>
	</view>
  <view class="cu-form-group margin-top">
		<view class="title">体温℃</view>
		<input name="tem"  placeholder="请输入体温" ></input>
	</view>
  <view class="padding flex flex-direction">
    <button form-type='submit' class="cu-btn bg-green lg" role="button" disabled="{{disabled}}" aria-disabled="false">提交</button>
  </view> 
</form>
