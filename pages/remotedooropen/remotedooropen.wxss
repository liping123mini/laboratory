.myscroll .list .left {
	width: 120rpx;
	height: 120rpx;
	padding: 30rpx;
	border-radius: 50%;
	box-sizing: border-box;
	box-shadow: 0rpx 0rpx 5rpx 5rpx #dddddd;
}
.myscroll .list .right {
	width: 70%;
}
.myscroll .list .left.iconfont {
	width: fit-content;
	margin-top: 0rpx;
	font-weight: 600;
}