.box {
	padding: 20rpx;
}
.mytab {
	margin-bottom: 30rpx;
	background: #fff;
	justify-content: space-around;
}
.mytab text {
	padding: 0rpx 10rpx;
	line-height: 100rpx;
}
.cur {
	border-bottom: 4rpx solid #39b54a;
}
.nlist {
	top: 300rpx;
	color: #999;
	font-size: 32rpx;
	position: absolute;
}
.myscroll .list {
	width: 100%;
	padding: 20rpx;
	border-radius: 20rpx;
	background: #fff;
	overflow: hidden;
	position: relative;
	margin-bottom: 25rpx;
}
.myscroll .list1 text{
	line-height: 50rpx;
}
.myscroll .list .top {
	font-weight: 550;
	align-items: center;
	margin-bottom: 45rpx;
}

