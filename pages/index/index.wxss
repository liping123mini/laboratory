/**index.wxss*/

page {
	background: white;
}

.myscroll {
	white-space: nowrap;
	width: 90%;
	margin: auto;
	height: 225rpx;
	border-bottom: 1rpx solid #e3e3e3;
}

.myscroll .single {
	display: inline-block;
	/* width: 33.3%!important; */
	text-align: center;
}

.single text {
	color: #999;
	font-size: 24rpx;
	display: block;
	width: 225rpx;
	padding-top: 20rpx;
}

.cuIcon-barcode {
	font-size: 140rpx;
	line-height: 190rpx;
}

.code {
	width: 200rpx;
	height: 200rpx;
	border-radius: 50%;
	margin: 0 auto;
	margin-top: 100rpx;
}

.codefont {
	margin-top: -40rpx;
}

.font {
	margin-top: 35rpx;
	font-size: 30rpx;

}

.text-green {
	margin-left: 10rpx;
	font-weight: bold;
}
